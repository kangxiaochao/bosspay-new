<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hyfd.dao.mp.PhonePhysicalchannelDao">
    <select id="selectCount" resultType="java.lang.Integer">
        select count(id) from mp_specify_section_recharge
    </select>

    <select id="selectAll" resultType="java.util.Map">
        select * from mp_specify_section_recharge
        <where>
        <if test="section != null and section != ''">
            and section like concat('%',#{section,jdbcType=VARCHAR},'%')
        </if>
        <if test="dispatcher_provider_id != null and dispatcher_provider_id !=''">
            and dispatcher_provider_id = #{dispatcher_provider_id,jdbcType=VARCHAR}
        </if>
        </where>
    </select>

    <insert id="phonePhysicalchanneladd" parameterType="java.util.Map">
        insert into mp_specify_section_recharge (id,dispatcher_provider_id,section,create_time) values (#{id,jdbcType=VARCHAR}, #{dispatcher_provider_id,jdbcType=VARCHAR},#{section,jdbcType=VARCHAR},CURRENT_TIMESTAMP)
    </insert>
    <delete id="deleteByPrimaryKey" parameterType="string">
       delete from mp_specify_section_recharge where id = #{id,jdbcType=VARCHAR}
    </delete>
    <select id="selectByPrimaryKey" parameterType="string" resultType="map">
        select * from mp_specify_section_recharge where id = #{id,jdbcType=VARCHAR}
    </select>

    <update id="updateByPrimaryKeySelective" parameterType="map">
        update mp_specify_section_recharge
        <set>
            <if test="section != null and section !=''">
                section = #{section,jdbcType=VARCHAR},
            </if>
            <if test="dispatcher_provider_id != null and dispatcher_provider_id !=''">
                dispatcher_provider_id = #{dispatcher_provider_id,jdbcType=VARCHAR},
            </if>
        </set>
        where id = #{id,jdbcType=VARCHAR}
    </update>
</mapper>